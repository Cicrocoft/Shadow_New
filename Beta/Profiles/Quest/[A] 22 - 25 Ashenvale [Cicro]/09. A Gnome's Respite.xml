dition="IsEquippedItemType('Crossbow') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Sharp Arrow" NpcName="Haljan Oakheart" UnderAmount="100" BuyAmount="1000"/>	</If>	<!-- End Restock -->	<!-- Pick up -->    <If Condition="Data.HasActiveQuest('On Guard in Stonetalon', 1070) == false and Data.HasCompletedQuest('On Guard in Stonetalon', 1070) == false">        <TakeQuest NpcName="Sentinel Thenysil" QuestId="1070" QuestName="On Guard in Stonetalon" />    </If>		    <If Condition="Data.HasActiveQuest('Super Reaper 6000') == false and Data.HasCompletedQuest('Super Reaper 6000') == false">        <TakeQuest NpcName="Ziz Fizziks" QuestId="1093" QuestName="Super Reaper 6000" />    </If>	    <If Condition="Data.HasActiveQuest('On Guard in Stonetalon', 1070) == true">        <ReturnQuest NpcName="Kaela Shadowspear" QuestId="1070" QuestName="On Guard in Stonetalon" />    </If>    <If Condition="Data.HasActiveQuest('On Guard in Stonetalon', 1085) == false and Data.HasCompletedQuest('On Guard in Stonetalon', 1085) == false and Data.HasCompletedQuest('On Guard in Stonetalon', 1070) == true">        <TakeQuest NpcName="Kaela Shadowspear" QuestId="1085" QuestName="On Guard in Stonetalon" />    </If>    <If Condition="Data.HasActiveQuest('On Guard in Stonetalon', 1085) == true">        <ReturnQuest NpcName="Gaxim Rustfizzle" QuestId="1085" QuestName="On Guard in Stonetalon" />    </If>    <If Condition="Data.HasActiveQuest('A Gnome\'s Respite') == false and Data.HasCompletedQuest('A Gnome\'s Respite') == false and Data.HasCompletedQuest('On Guard in Stonetalon', 1085) == true">        <TakeQuest NpcName="Gaxim Rustfizzle" QuestId="1071" QuestName="A Gnome\'s Respite" />    </If>		<If Condition="Data.HasActiveQuest('Super Reaper 6000') == true and Data.AllQuestObjectivesAreComplete('Super Reaper 6000') == false" >		<ClearHotspots />		<!-- Settings -->		<SetTargetingMode Mode="Around Points" />		<!-- Blackspots -->		<AddBl
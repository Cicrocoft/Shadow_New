<ShadowProfile Condition="Data.HasCompletedQuest('Pridewings of Stonetalon') == false and Data.HasCompletedQuest('The Zoram Strand') == true">
	<!-- Bot settings -->	<SetBaseSettings />    <SetEnableVendoring Value="true"/>    <SetVendorNpc NpcName="Veenix"/>    <SetVendorSlotsCount Value="2" />    <SetEnableRepairing Value="true"/>    <SetRepairNpc NpcName="Veenix"/>    <SetRepairPercent Value="40" />	<SetMaxZDifference Value="5" />	<!-- Restock Ashenvale 22+ (Astranaar)-->	<SetEnableRestockItems Value="true"/>	<ClearRestockItems/>	<!-- Food -->	<If Condition="Data.PlayerClass() == WoWClass.Rogue or Data.PlayerClass() == WoWClass.Warrior or Data.PlayerClass() == WoWClass.Warlock">		<AddRestockItem ItemName="Tel\'Abim Banana" NpcName="Maliynn" UnderAmount="10" BuyAmount="80"/>		<AddDoNotSellItem ItemName="Tel\'Abim Banana" />	</If>	<If Condition="Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Tel\'Abim Banana" NpcName="Maliynn" UnderAmount="10" BuyAmount="120"/>		<AddDoNotSellItem ItemName="Tel\'Abim Banana" />		<Lua>settings.ShouldFeedPet = true</Lua>		<Lua>settings.ShouldFeedPetFoodsStr = "Tel\'Abim Banana" </Lua>	</If>		<!-- Water -->	<If Condition="Data.PlayerClass() == WoWClass.Druid or Data.PlayerClass() == WoWClass.Hunter or Data.PlayerClass() == WoWClass.Warlock or Data.PlayerClass() == WoWClass.Paladin">		<AddRestockItem ItemName="Ice Cold Milk" NpcName="Maliynn" UnderAmount="5" BuyAmount="60"/>		<AddDoNotSellItem ItemName="Ice Cold Milk" />	</If>	<If Condition="Data.PlayerClass() == WoWClass.Priest">		<AddRestockItem ItemName="Ice Cold Milk" NpcName="Maliynn" UnderAmount="3" BuyAmount="40"/>		<AddDoNotSellItem ItemName="Ice Cold Milk" />	</If>	<!-- Bullets/Arrows -->	<If Condition="IsEquippedItemType('Gun') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Heavy Shot" NpcName="Haljan Oakheart" UnderAmount="100" BuyAmount="1000"/>	</If>	<If Condition="IsEquippedItemType('Bow') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Sharp Arrow" NpcName="Haljan Oakheart" UnderAmount="100" BuyAmount="1000"/>	</If>	<If Condition="IsEquippedItemType('Crossbow') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Sharp Arrow" NpcName="Haljan Oakheart" UnderAmount="100" BuyAmount="1000"/>	</If>	<!-- End Restock -->	<!-- Pick up -->    <If Condition="Data.HasActiveQuest('Pridewings of Stonetalon') == false and Data.HasCompletedQuest('Pridewings of Stonetalon') == false">        <TakeQuest NpcName="Shindrell Swiftfire" QuestId="1134" QuestName="Pridewings of Stonetalon" />    </If>	<If Condition="Data.HasActiveQuest('Pridewings of Stonetalon') == true and Data.AllQuestObjectivesAreComplete('Pridewings of Stonetalon') == false" >		<ClearHotspots />		<!-- Settings -->		<SetTargetingMode Mode="Around Points" />		<!-- Blackspots -->		<AddBlackspot X="1210.5041503906" Y="-22.767539978027" Z="-3.1320579051971" Range="30" />		<AddBlackspot X="1112.0943603516" Y="181.67344665527" Z="1.9583320617676" Range="30" />		<!-- Hotspots -->		<AddHotspot X="1433.4088134766" Y="736.53552246094" Z="143.5588684082" Range="100" />		<AddHotspot X="1514.5415039063" Y="598.59320068359" Z="144.0650177002" Range="100" />		<AddHotspot X="1620.1739501953" Y="612.25323486328" Z="175.04107666016" Range="100" />		<AddHotspot X="1670.6590576172" Y="580.10467529297" Z="194.53814697266" Range="100" />		<AddHotspot X="1686.1712646484" Y="649.30096435547" Z="195.1205291748" Range="100" />		<AddHotspot X="1751.3817138672" Y="628.83770751953" Z="194.97235107422" Range="100" />		<!-- Mob Whitelist -->		<ClearMonsterFilter/>		<SetMonsterFilterMode Mode="Whitelist" />		<AddMonsterFilterName Value="Pridewing Skyhunter" />			<AddMonsterFilterName Value="Pridewing Consort" />			<AddMonsterFilterName Value="Pridewing Wyvern" />			<AddMonsterFilterName Value="Young Pridewing" />					<While Condition="Data.AllQuestObjectivesAreComplete('Pridewings of Stonetalon') == false" >				<Wait Value="100" />		</While>		</If>	<!-- ReturnQuest -->    <If Condition="Data.HasActiveQuest('Pridewings of Stonetalon') == true and Data.AllQuestObjectivesAreComplete('Pridewings of Stonetalon') == true">        <ReturnQuest NpcName="Shindrell Swiftfire" QuestId="1134" QuestName="Pridewings of Stonetalon" />    </If>	    <If Condition="Data.HasActiveQuest('Castpipe\'s Task') == false and Data.HasCompletedQuest('Castpipe\'s Task') == false and Data.HasCompletedQuest('A Gnome\'s Respite') == true and UnitLevel('player') &gt;= 25">        <TakeQuest NpcName="Gaxim Rustfizzle" QuestId="2931" QuestName="Castpipe\'s Task" />    </If>	
</ShadowProfile>
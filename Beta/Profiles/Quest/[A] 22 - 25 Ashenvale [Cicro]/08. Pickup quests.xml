<ShadowProfile Condition="UnitLevel('player') &gt;= 24 and Data.HasCompletedQuest('On Guard in Stonetalon', 1085) == false">
	<!-- Bot settings -->	<SetBaseSettings />    <SetEnableVendoring Value="true"/>    <SetVendorNpc NpcName="Veenix"/>    <SetVendorSlotsCount Value="2" />    <SetEnableRepairing Value="true"/>    <SetRepairNpc NpcName="Veenix"/>    <SetRepairPercent Value="40" />	<!-- Restock Ashenvale 22+ (Astranaar)-->	<SetEnableRestockItems Value="true"/>	<ClearRestockItems/>	<!-- Food -->	<If Condition="Data.PlayerClass() == WoWClass.Rogue or Data.PlayerClass() == WoWClass.Warrior or Data.PlayerClass() == WoWClass.Warlock">		<AddRestockItem ItemName="Tel\'Abim Banana" NpcName="Maliynn" UnderAmount="10" BuyAmount="80"/>		<AddDoNotSellItem ItemName="Tel\'Abim Banana" />	</If>	<If Condition="Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Tel\'Abim Banana" NpcName="Maliynn" UnderAmount="10" BuyAmount="120"/>		<AddDoNotSellItem ItemName="Tel\'Abim Banana" />		<Lua>settings.ShouldFeedPet = true</Lua>		<Lua>settings.ShouldFeedPetFoodsStr = "Tel\'Abim Banana" </Lua>	</If>		<!-- Water -->	<If Condition="Data.PlayerClass() == WoWClass.Druid or Data.PlayerClass() == WoWClass.Hunter or Data.PlayerClass() == WoWClass.Warlock or Data.PlayerClass() == WoWClass.Paladin">		<AddRestockItem ItemName="Ice Cold Milk" NpcName="Maliynn" UnderAmount="5" BuyAmount="60"/>		<AddDoNotSellItem ItemName="Ice Cold Milk" />	</If>	<If Condition="Data.PlayerClass() == WoWClass.Priest">		<AddRestockItem ItemName="Ice Cold Milk" NpcName="Maliynn" UnderAmount="3" BuyAmount="40"/>		<AddDoNotSellItem ItemName="Ice Cold Milk" />	</If>	<!-- Bullets/Arrows -->	<If Condition="IsEquippedItemType('Gun') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Heavy Shot" NpcName="Haljan Oakheart" UnderAmount="100" BuyAmount="1000"/>	</If>	<If Condition="IsEquippedItemType('Bow') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Sharp Arrow" NpcName="Haljan Oakheart" UnderAmount="100" BuyAmount="1000"/>	</If>	<If Condition="IsEquippedItemType('Crossbow') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Sharp Arrow" NpcName="Haljan Oakheart" UnderAmount="100" BuyAmount="1000"/>	</If>	<!-- End Restock -->				<AddBlackspot X="1210.5041503906" Y="-22.767539978027" Z="-3.1320579051971" Range="40" />
	<!-- Pickup -->    <If Condition="Data.HasActiveQuest('Pridewings of Stonetalon') == false and Data.HasCompletedQuest('Pridewings of Stonetalon') == false and Data.HasCompletedQuest('The Zoram Strand') == true">        <TakeQuest NpcName="Shindrell Swiftfire" QuestId="1134" QuestName="Pridewings of Stonetalon" />    </If>    <If Condition="Data.HasActiveQuest('On Guard in Stonetalon', 1070) == false and Data.HasCompletedQuest('On Guard in Stonetalon', 1070) == false">        <TakeQuest NpcName="Sentinel Thenysil" QuestId="1070" QuestName="On Guard in Stonetalon" />    </If>		    <If Condition="Data.HasActiveQuest('Super Reaper 6000') == false and Data.HasCompletedQuest('Super Reaper 6000') == false">        <TakeQuest NpcName="Ziz Fizziks" QuestId="1093" QuestName="Super Reaper 6000" />    </If>	    <If Condition="Data.HasActiveQuest('On Guard in Stonetalon', 1070) == true">        <ReturnQuest NpcName="Kaela Shadowspear" QuestId="1070" QuestName="On Guard in Stonetalon" />    </If>    <If Condition="Data.HasActiveQuest('On Guard in Stonetalon', 1085) == false and Data.HasCompletedQuest('On Guard in Stonetalon', 1085) == false and Data.HasCompletedQuest('On Guard in Stonetalon', 1070) == true">        <TakeQuest NpcName="Kaela Shadowspear" QuestId="1085" QuestName="On Guard in Stonetalon" />    </If>
    <If Condition="Data.HasActiveQuest('On Guard in Stonetalon', 1085) == true">        <ReturnQuest NpcName="Gaxim Rustfizzle" QuestId="1085" QuestName="On Guard in Stonetalon" />    </If>    <If Condition="Data.HasActiveQuest('A Gnome\'s Respite') == false and Data.HasCompletedQuest('A Gnome\'s Respite') == false and Data.HasCompletedQuest('On Guard in Stonetalon', 1085) == true">        <TakeQuest NpcName="Gaxim Rustfizzle" QuestId="1071" QuestName="A Gnome\'s Respite" />    </If>
</ShadowProfile>
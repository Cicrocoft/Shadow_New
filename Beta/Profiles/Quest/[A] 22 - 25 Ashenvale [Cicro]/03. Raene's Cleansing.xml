<ShadowProfile Condition="Data.HasCompletedQuest('Raene\'s Cleansing', 1023) == false">

	<!-- Bot settings -->	<SetBaseSettings />    <SetEnableVendoring Value="true"/>    <SetVendorNpc NpcName="Xai\'ander"/>    <SetVendorSlotsCount Value="2" />    <SetEnableRepairing Value="true"/>    <SetRepairNpc NpcName="Xai\'ander"/>    <SetRepairPercent Value="35" />	<!-- Restock Ashenvale 22+ (Astranaar)-->	<SetEnableRestockItems Value="true"/>	<ClearRestockItems/>	<!-- Food -->	<If Condition="Data.PlayerClass() == WoWClass.Rogue or Data.PlayerClass() == WoWClass.Warrior or Data.PlayerClass() == WoWClass.Warlock">		<AddRestockItem ItemName="Tel\'Abim Banana" NpcName="Maliynn" UnderAmount="5" BuyAmount="40"/>		<AddDoNotSellItem ItemName="Tel\'Abim Banana" />	</If>	<If Condition="Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Tel\'Abim Banana" NpcName="Maliynn" UnderAmount="5" BuyAmount="60"/>		<AddDoNotSellItem ItemName="Tel\'Abim Banana" />		<Lua>settings.ShouldFeedPet = true</Lua>		<Lua>settings.ShouldFeedPetFoodsStr = "Tel\'Abim Banana" </Lua>	</If>		<!-- Water -->	<If Condition="Data.PlayerClass() == WoWClass.Druid or Data.PlayerClass() == WoWClass.Hunter or Data.PlayerClass() == WoWClass.Warlock or Data.PlayerClass() == WoWClass.Paladin">		<AddRestockItem ItemName="Ice Cold Milk" NpcName="Maliynn" UnderAmount="5" BuyAmount="40"/>		<AddDoNotSellItem ItemName="Ice Cold Milk" />	</If>	<If Condition="Data.PlayerClass() == WoWClass.Priest">		<AddRestockItem ItemName="Ice Cold Milk" NpcName="Maliynn" UnderAmount="3" BuyAmount="20"/>		<AddDoNotSellItem ItemName="Ice Cold Milk" />	</If>	<!-- Bullets/Arrows -->	<If Condition="IsEquippedItemType('Gun') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Heavy Shot" NpcName="Haljan Oakheart" UnderAmount="100" BuyAmount="600"/>	</If>	<If Condition="IsEquippedItemType('Bow') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Sharp Arrow" NpcName="Haljan Oakheart" UnderAmount="100" BuyAmount="600"/>	</If>	<If Condition="IsEquippedItemType('Crossbow') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Sharp Arrow" NpcName="Haljan Oakheart" UnderAmount="100" BuyAmount="600"/>	</If>	<!-- End Restock -->	<!-- Pick up -->	<If Condition="Data.HasActiveQuest('Raene\'s Cleansing', 991) == false and Data.HasCompletedQuest('Raene\'s Cleansing', 991) == false">        <TakeQuest NpcName="Raene Wolfrunner" QuestId="991" QuestName="Raene\'s Cleansing" />    </If>	<If Condition="Data.HasActiveQuest('Raene\'s Cleansing', 991) == true">        <ReturnQuest NpcName="Teronis\' Corpse" QuestId="991" QuestName="Raene\'s Cleansing" />    </If> 		<If Condition="Data.HasActiveQuest('Raene\'s Cleansing', 1023) == false and Data.HasCompletedQuest('Raene\'s Cleansing', 1023) == false and Data.HasCompletedQuest('Raene\'s Cleansing', 991) == true">        <TakeQuest NpcName="Teronis\' Corpse" QuestId="1023" QuestName="Raene\'s Cleansing" />    </If>			<If Condition="Data.HasActiveQuest('Raene\'s Cleansing', 1023) == true and Data.AllQuestObjectivesAreComplete('Raene\'s Cleansing', 1023) == false" >		<ClearHotspots />		<!-- Settings -->		<SetTargetingMode Mode="Around Points" />		<!-- Blackspots -->		<!-- Hotspots -->		<AddHotspot X="3042.4113769531" Y="530.15747070313" Z="5.1063528060913" Range="100" />		<!-- Mob Whitelist -->		<ClearMonsterFilter/>		<While Condition="Data.AllQuestObjectivesAreComplete('Raene\'s Cleansing', 1023) == false" >				<Wait Value="100" />		</While>		</If>	<!-- ReturnQuest -->	<If Condition="Data.HasActiveQuest('Raene\'s Cleansing', 1023) == true and Data.AllQuestObjectivesAreComplete('Raene\'s Cleansing', 1023) == true">        <ReturnQuest NpcName="Raene Wolfrunner" QuestId="1023" QuestName="Raene\'s Cleansing" />    </If> 	
</ShadowProfile>
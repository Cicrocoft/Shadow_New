	<If Condition="Data.HasActiveQuest('Pie for Billy') == false and Data.HasCompletedQuest('Pie for Billy') == false and Data.HasCompletedQuest('Lost Necklace') == true">		<TakeQuest NpcName="Billy Maclure" QuestId="86" QuestName="Pie for Billy" />	</If>	
	<!-- Quset Completion -->

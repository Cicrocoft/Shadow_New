		<ClearMonsterFilter/>		<SetMonsterFilterMode Mode="Whitelist" />		<AddMonsterFilterName Value="Stonetusk Boar" />			<While Condition="Data.AllQuestObjectivesAreComplete('Pie for Billy') == false" >			<Wait Value="100" />		</While>		<ClearMonsterFilter/>		<SetMonsterFilterMode Mode="None" />		<While Condition="UnitLevel('player') &lt; 7 or UnitXP('player') &lt;= 800 and UnitXPMax('player') == 4500" >			<Wait Value="100" />		</While>			</If>
	<!-- ReturnQuest -->
    <If Condition="Data.HasActiveQuest('Pie for Billy') == true and Data.AllQuestObjectivesAreComplete('Pie for Billy') == true">			<ReturnQuest NpcName="Auntie Bernice Stonefield" QuestId="86" QuestName="Pie for Billy" />	</If>	
	<If Condition="Data.HasActiveQuest('Back to Billy') == false and Data.HasCompletedQuest('Back to Billy') == false and Data.HasCompletedQuest('Pie for Billy') == true">		<TakeQuest NpcName="Auntie Bernice Stonefield" QuestId="84" QuestName="Back to Billy" />	</If>		
    <If Condition="Data.HasActiveQuest('Back to Billy') == true and Data.AllQuestObjectivesAreComplete('Back to Billy') == true">			<ReturnQuest NpcName="Billy Maclure" QuestId="84" QuestName="Back to Billy" />	</If>
	<If Condition="Data.HasActiveQuest('Goldtooth') == false and Data.HasCompletedQuest('Goldtooth') == false and Data.HasCompletedQuest('Back to Billy') == true">		<TakeQuest NpcName="Billy Maclure" QuestId="87" QuestName="Goldtooth" />	</If>	
</ShadowProfile>
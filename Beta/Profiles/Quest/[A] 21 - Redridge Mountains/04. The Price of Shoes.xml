<ShadowProfile Condition="Data.HasCompletedQuest('The Price of Shoes') == false">
	<!-- Bot settings -->	<SetBaseSettings />    <SetEnableVendoring Value="true"/>    <SetVendorNpc NpcName="Kara Adams"/>    <SetVendorSlotsCount Value="1" />    <SetEnableRepairing Value="true"/>    <SetRepairNpc NpcName="Kara Adams"/>    <SetRepairPercent Value="40" />
	<!-- Pickup -->
	    <If Condition="Data.HasActiveQuest('The Price of Shoes') == false and Data.HasCompletedQuest('The Price of Shoes') == false">        <TakeQuest NpcName="Verner Osgood" QuestId="118" QuestName="The Price of Shoes" />    </If>		    <If Condition="Data.HasActiveQuest('The Price of Shoes') == true">        <ReturnQuest NpcName="Smith Argus" QuestId="118" QuestName="The Price of Shoes" />    </If> 	<If Condition="Data.HasActiveQuest('Return to Verner') == false and Data.HasCompletedQuest('Return to Verner') == false and Data.HasCompletedQuest('The Price of Shoes') == true">        <TakeQuest NpcName="Smith Argus" QuestId="119" QuestName="Return to Verner" />    </If>    <If Condition="Data.HasActiveQuest('Return to Verner') == true">        <ReturnQuest NpcName="Verner Osgood" QuestId="119" QuestName="Return to Verner" />    </If> 		
</ShadowProfile>
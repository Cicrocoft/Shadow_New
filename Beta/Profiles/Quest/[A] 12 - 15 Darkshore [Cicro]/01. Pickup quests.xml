<ShadowProfile Condition="UnitLevel('player') &gt;= 12 and Data.HasCompletedQuest('Washed Ashore', 3524) == false">
	<!-- Bot settings -->	<SetBaseSettings />    <SetEnableVendoring Value="true"/>    <SetVendorNpc NpcName="Elisa Steelhand"/>    <SetVendorSlotsCount Value="2" />    <SetEnableRepairing Value="true"/>    <SetRepairNpc NpcName="Elisa Steelhand"/>    <SetRepairPercent Value="35" />	<!-- Restock Darkshore 12+ -->	<SetEnableRestockItems Value="true"/>	<ClearRestockItems/>	<!-- Food -->	<If Condition="Data.PlayerClass() == WoWClass.Rogue or Data.PlayerClass() == WoWClass.Warrior or Data.PlayerClass() == WoWClass.Warlock or Data.PlayerClass() == WoWClass.Paladin or Data.PlayerClass() == WoWClass.Priest">		<AddRestockItem ItemName="Longjaw Mud Snapper" NpcName="Laird" UnderAmount="5" BuyAmount="40"/>		<AddDoNotSellItem ItemName="Longjaw Mud Snapper" />	</If>	<If Condition="Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Longjaw Mud Snapper" NpcName="Laird" UnderAmount="5" BuyAmount="60"/>		<AddDoNotSellItem ItemName="Longjaw Mud Snapper" />		<Lua>settings.ShouldFeedPet = true</Lua>		<Lua>settings.ShouldFeedPetFoodsStr = "Longjaw Mud Snapper" </Lua>			</If>			<!-- Water -->	<If Condition="Data.PlayerClass() == WoWClass.Druid or Data.PlayerClass() == WoWClass.Hunter or Data.PlayerClass() == WoWClass.Warlock or Data.PlayerClass() == WoWClass.Paladin">		<AddRestockItem ItemName="Refreshing Spring Water" NpcName="Taldan" UnderAmount="5" BuyAmount="40"/>		<AddDoNotSellItem ItemName="Refreshing Spring Water" />	</If>	<If Condition="Data.PlayerClass() == WoWClass.Priest">		<AddRestockItem ItemName="Refreshing Spring Water" NpcName="Taldan" UnderAmount="3" BuyAmount="20"/>		<AddDoNotSellItem ItemName="Refreshing Spring Water" />	</If>	<!-- Bullets/Arrows -->	<If Condition="IsEquippedItemType('Gun') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Heavy Shot" NpcName="Dalmond" UnderAmount="100" BuyAmount="800"/>	</If>	<If Condition="IsEquippedItemType('Bow') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Sharp Arrow" NpcName="Dalmond" UnderAmount="100" BuyAmount="800"/>	</If>	<If Condition="IsEquippedItemType('Crossbow') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Sharp Arrow" NpcName="Dalmond" UnderAmount="100" BuyAmount="800"/>	</If>	<!-- End Restock -->
	<!-- Pickup --><If Condition="Data.HasActiveQuest('Tools of the Highborne') == false and Data.HasCompletedQuest('Tools of the Highborne') == false">
	<TakeQuest NpcName="Gwennyth Bly\'Leggonde" QuestId="3524" QuestName="Washed Ashore" />	<TakeQuest NpcName="Wizbang Cranktoggle" QuestId="983" QuestName="Buzzbox 827" />	<TakeQuest NpcName="Cerellean Whiteclaw" QuestId="963" QuestName="For Love Eternal" />	<TakeQuest NpcName="Sentinel Glynda Nal'Shea" QuestId="4811" QuestName="The Red Crystal" />	<TakeQuest NpcName="Tharnariun Treetender" QuestId="2118" QuestName="Plagued Lands" />	<TakeQuest NpcName="Terenthis" QuestId="984" QuestName="How Big a Threat?" />	<TakeQuest NpcName="Thundris Windweaver" QuestId="954" QuestName="Bashal\'Aran" />	<TakeQuest NpcName="Thundris Windweaver" QuestId="958" QuestName="Tools of the Highborne" /></If>

</ShadowProfile>
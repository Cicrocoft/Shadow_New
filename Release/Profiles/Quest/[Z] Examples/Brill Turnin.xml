<ShadowProfile Condition="not Data.HasCompletedQuest('Graverobbers') ">
    <SetBaseSettings />

    <ReturnQuest NpcName="Junior Apothecary Holland" QuestName="Doom Weed" />

    <ReturnQuest NpcName="Apothecary Johaan" QuestId="368" QuestName="A New Plague" />
    <If Condition="Data.HasCompletedQuest(nil, 369) and not Data.HasActiveQuest(nil, 369)">
        <TakeQuest NpcName="Apothecary Johaan" QuestId="369" QuestName="A New Plague" />
    </If>
    <If Condition="not Data.HasActiveQuest('Delivery to Silverpine Forest')">
        <TakeQuest NpcName="Apothecary Johaan" QuestName="Delivery to Silverpine Forest" />
    </If>

    <ReturnQuest NpcName="Executor Zygand" QuestName="Wanted: Maggot Eye" />

    <ReturnQuest NpcName="Magistrate Sevren" QuestName="Graverobbers" />

</ShadowProfile>
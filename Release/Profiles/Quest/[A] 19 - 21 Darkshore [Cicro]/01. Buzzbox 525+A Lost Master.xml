<ShadowProfile Condition="Data.HasCompletedQuest('A Lost Master', 993) == false or Data.HasCompletedQuest('The Absent Minded Prospector') == false or Data.HasCompletedQuest('Buzzbox 525') == false">

	<!-- Bot settings -->
	<SetBaseSettings />
    <SetEnableVendoring Value="true"/>
    <SetVendorNpc NpcName="Elisa Steelhand"/>
    <SetVendorSlotsCount Value="2" />
    <SetEnableRepairing Value="true"/>
    <SetRepairNpc NpcName="Elisa Steelhand"/>
    <SetRepairPercent Value="40" />
	
	<!-- Restock Darkshore 12+ -->
	<SetEnableRestockItems Value="true"/>
	<ClearRestockItems />
	<!-- Food -->
	<If Condition="Data.PlayerClass() == WoWClass.Rogue or Data.PlayerClass() == WoWClass.Warrior or Data.PlayerClass() == WoWClass.Hunter or Data.PlayerClass() == WoWClass.Warlock">
		<AddRestockItem ItemName="Longjaw Mud Snapper" NpcName="Laird" UnderAmount="5" BuyAmount="40"/>
		<AddDoNotSellItem ItemName="Longjaw Mud Snapper" />
	</If>
	<!-- Water -->
	<If Condition="Data.PlayerClass() == WoWClass.Druid or Data.PlayerClass() == WoWClass.Hunter or Data.PlayerClass() == WoWClass.Warlock or Data.PlayerClass() == WoWClass.Paladin">
		<AddRestockItem ItemName="Refreshing Spring Water" NpcName="Taldan" UnderAmount="5" BuyAmount="40"/>
		<AddDoNotSellItem ItemName="Refreshing Spring Water" />
	</If>
	<If Condition="Data.PlayerClass() == WoWClass.Priest">
		<AddRestockItem ItemName="Refreshing Spring Water" NpcName="Taldan" UnderAmount="3" BuyAmount="20"/>
		<AddDoNotSellItem ItemName="Refreshing Spring Water" />
	</If>
	<!-- Bullets/Arrows -->
	<If Condition="IsEquippedItemType('Gun') == true and Data.PlayerClass() == WoWClass.Hunter">
		<AddRestockItem ItemName="Heavy Shot" NpcName="Dalmond" UnderAmount="100" BuyAmount="800"/>
	</If>
	<If Condition="IsEquippedItemType('Bow') == true and Data.PlayerClass() == WoWClass.Hunter">
		<AddRestockItem ItemName="Sharp Arrow" NpcName="Dalmond" UnderAmount="100" BuyAmount="800"/>
	</If>
	<If Condition="IsEquippedItemType('Crossbow') == true and Data.PlayerClass() == WoWClass.Hunter">
		<AddRestockItem ItemName="Sharp Arrow" NpcName="Dalmond" UnderAmount="100" BuyAmount="800"/>
	</If>
	<!-- End Restock -->


	<!-- Pickup -->	
	<If Condition="Data.HasActiveQuest('Buzzbox 525') == false and Data.HasCompletedQuest('Buzzbox 323') == true and Data.HasCompletedQuest('Buzzbox 525') == false">
		<TakeQuestFromObject QuestName="Buzzbox 525" X="7258.3291015625" Y="-416.59912109375" Z="26.949256896973" />
	</If>	
     <If Condition="Data.HasActiveQuest('The Absent Minded Prospector') == false and Data.HasCompletedQuest('The Absent Minded Prospector') == false">
        <TakeQuest  NpcName="Archaeologist Hollee" QuestId="729" QuestName="The Absent Minded Prospector" />
    </If>  
     <If Condition="Data.HasActiveQuest('A Lost Master', 986) == false and Data.HasCompletedQuest('A Lost Master', 986) == false">
        <TakeQuest  NpcName="Terenthis" QuestId="986" QuestName="A Lost Master" />
    </If>    
	<!-- Objective Completion -->
	

	
    <If Condition="Data.HasActiveQuest('Buzzbox 525') == true and Data.AllQuestObjectivesAreComplete('Buzzbox 525') == false">
		<ClearHotspots />
		<!-- Settings -->
		<SetTargetingMode Mode="Around Points" />
		<!-- Blackspots -->
		<AddBlackspot X="4992.6118164063" Y="560.41522216797" Z="4.1801152229309" Range="40" />
		<!-- Hotspots -->
		<AddHotspot X="5331.1640625" Y="408.02490234375" Z="28.857032775879" Range="100" />
		<AddHotspot X="5263.76953125" Y="382.45529174805" Z="29.046649932861" Range="100" />
		<AddHotspot X="5160.3188476563" Y="395.39724731445" Z="31.232055664063" Range="100" />
		<AddHotspot X="5049.2646484375" Y="380.80523681641" Z="30.978189468384" Range="100" />
		<AddHotspot X="4905.337890625" Y="423.40936279297" Z="35.224418640137" Range="100" />
		<AddHotspot X="4800.2900390625" Y="442.60638427734" Z="32.031909942627" Range="100" />
		<AddHotspot X="4703.2612304688" Y="473.01602172852" Z="30.230484008789" Range="100" />
		<!-- Mob Whitelist -->
		<ClearMonsterFilter/>
		<While Condition="Data.AllQuestObjectivesAreComplete('Buzzbox 525') == false" >
			<Wait Value="100" />
		</While>
    </If>
	
    <If Condition="Data.HasActiveQuest('A Lost Master', 986) == true and Data.AllQuestObjectivesAreComplete('A Lost Master', 986) == false">
		<ClearHotspots />
		<!-- Settings -->
		<SetTargetingMode Mode="Around Points" />
		<!-- Blackspots -->
		<AddBlackspot X="4992.6118164063" Y="560.41522216797" Z="4.1801152229309" Range="40" />
		<!-- Hotspots -->
		<AddHotspot X="5331.1640625" Y="408.02490234375" Z="28.857032775879" Range="100" />
		<AddHotspot X="5263.76953125" Y="382.45529174805" Z="29.046649932861" Range="100" />
		<AddHotspot X="5160.3188476563" Y="395.39724731445" Z="31.232055664063" Range="100" />
		<AddHotspot X="5049.2646484375" Y="380.80523681641" Z="30.978189468384" Range="100" />
		<AddHotspot X="4905.337890625" Y="423.40936279297" Z="35.224418640137" Range="100" />
		<AddHotspot X="4800.2900390625" Y="442.60638427734" Z="32.031909942627" Range="100" />
		<AddHotspot X="4703.2612304688" Y="473.01602172852" Z="30.230484008789" Range="100" />
		<!-- Mob Whitelist -->
		<ClearMonsterFilter/>
		<While Condition="Data.AllQuestObjectivesAreComplete('A Lost Master', 986) == false" >
			<Wait Value="100" />
		</While>
    </If>	
	<!-- TurnIn -->
	
	<AddBlackspot X="4992.6118164063" Y="560.41522216797" Z="4.1801152229309" Range="40" />
     <If Condition="Data.HasActiveQuest('The Absent Minded Prospector') == true">
        <ReturnQuest  NpcName="Prospector Remtravel" QuestId="729" QuestName="The Absent Minded Prospector" />
    </If>  

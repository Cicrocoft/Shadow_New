<ShadowProfile Condition="Data.HasCompletedQuest('Frostmane Hold') == false or Data.HasCompletedQuest('The Reports') == false">
    <SetBaseSettings />	<!-- Restock Dun Morogh 5-12 (Kharanos, with hunter pet)-->	<SetEnableRestockItems Value="true"/>	<ClearRestockItems/>	<!-- Food -->	<If Condition="Data.PlayerClass() == WoWClass.Rogue or Data.PlayerClass() == WoWClass.Warrior or Data.PlayerClass() == WoWClass.Warlock">		<AddRestockItem ItemName="Tough Hunk of Bread" NpcName="Innkeeper Belm" UnderAmount="5" BuyAmount="40"/>		<AddDoNotSellItem ItemName="Tough Hunk of Bread" />	</If>	<If Condition="Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Tough Jerky" NpcName="Innkeeper Firebrew" UnderAmount="5" BuyAmount="60"/>		<AddDoNotSellItem ItemName="Tough Jerky" />		<Lua>settings.ShouldFeedPet = true</Lua>		<Lua>settings.ShouldFeedPetFoodsStr = "Tough Jerky" </Lua>	</If>	<!-- Water -->	<If Condition="Data.PlayerClass() == WoWClass.Druid or Data.PlayerClass() == WoWClass.Hunter or Data.PlayerClass() == WoWClass.Warlock or Data.PlayerClass() == WoWClass.Paladin">		<AddRestockItem ItemName="Refreshing Spring Water" NpcName="Innkeeper Belm" UnderAmount="5" BuyAmount="40"/>		<AddDoNotSellItem ItemName="Refreshing Spring Water" />	</If>	<If Condition="Data.PlayerClass() == WoWClass.Priest">		<AddRestockItem ItemName="Refreshing Spring Water" NpcName="Innkeeper Belm" UnderAmount="3" BuyAmount="20"/>		<AddDoNotSellItem ItemName="Refreshing Spring Water" />	</If>	<!-- Bullets/Arrows -->	<If Condition="IsEquippedItemType('Gun') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Light Shot" NpcName="Kreg Bilmn" UnderAmount="100" BuyAmount="1200"/>	</If>	<If Condition="IsEquippedItemType('Bow') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Rough Arrow" NpcName="Kreg Bilmn" UnderAmount="100" BuyAmount="1200"/>	</If>	<If Condition="IsEquippedItemType('Crossbow') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Rough Arrow" NpcName="Kreg Bilmn" UnderAmount="100" BuyAmount="1200"/>	</If>	<!-- End Restock -->
    <If Condition="Data.HasActiveQuest('Frostmane Hold') == false and Data.HasCompletedQuest('Frostmane Hold') == false">
        <TakeQuest NpcName="Senir Whitebeard" QuestName="Frostmane Hold" />
    </If>


	<SetBaseSettings />
    <SetEnableVendoring Value="true"/>
    <SetVendorNpc NpcName="Keeg Gibn"/>
    <SetVendorSlotsCount Value="2" />
    <SetEnableRepairing Value="true"/>
    <SetRepairNpc NpcName="Burdrak Harglhelm"/>
    <SetRepairPercent Value="35" />
    <If Condition="Data.HasActiveQuest('Frostmane Hold') == true and Data.HasCompletedQuest('Frostmane Hold') == false">	
		<ClearHotspots /><AddBlackspot X="-5320.4565429688" Y="158.76176452637" Z="390.14410400391" Range="40" /><AddBlackspot X="-5249.099609375" Y="101.77604675293" Z="391.97369384766" Range="50" />
			<AddBlackspot X="-5570.404296875" Y="512.13232421875" Z="381.84985351563" Range="45" />
			<AddHotspot X="-5557.8999023438" Y="642.83795166016" Z="399.37362670898" Range="20" />
			<AddHotspot X="-5553.318359375" Y="570.30773925781" Z="394.33544921875" Range="20" />
		<SetTargetingMode Mode="Around Points" />
		<ClearMonsterFilter/>
		<SetMonsterFilterMode Mode="Whitelist" />
		<AddMonsterFilterName Value="Frostmane Headhunter" />	
		<While Condition="Data.QuestObjectiveIsComplete('Frostmane Hold', 1) == false and Data.HasCompletedQuest('Frostmane Hold') == false" >
			<Wait Value="100" />
		</While>	
		<While Condition="Data.HasActiveQuest('Frostmane Hold') == true and Data.QuestObjectiveIsComplete('Frostmane Hold', 2) == false and Data.HasCompletedQuest('Frostmane Hold') == false">
			<ClearHotspots />
				<AddBlackspot X="-5570.404296875" Y="512.13232421875" Z="381.84985351563" Range="45" />
				<MoveTo X="-5590.2626953125" Y="658.24212646484" Z="394.52560424805" Range="3" />				<SetRepairPercent Value="5" />				<SetEnablePreemptiveMonsterAttack Value="false" />				<SetEnableProfileIgnoreCombat Value="true" />				<MoveTo NoNav="true" X="-5605.8286132813" Y="656.81518554688" Z="384.31747436523" Range="3" />
				<AddHotspot X="-5584.5493164063" Y="679.65185546875" Z="385.05163574219" Range="5" />
				<AddHotspot X="-5578.2392578125" Y="690.76293945313" Z="382.54428100586" Range="5" />		

		</While>			<If Condition="Data.HasActiveQuest('Frostmane Hold') == true and Data.QuestObjectiveIsComplete('Frostmane Hold', 2) == true">				<SetRepairPercent Value="35" />				<SetEnableProfileIgnoreCombat Value="false" />				<SetResurrectionMode Mode="SpiritHealer" />				<SetEnablePreemptiveMonsterAttack Value="true" />				<SetEnableVendoring Value="true"/>				<SetVendorNpc NpcName="Innkeeper Belm"/>				<SetVendorSlotsCount Value="2" />				<SetEnableRepairing Value="true"/>				<SetRepairNpc NpcName="Grawn Thromwyn"/>				<SetRepairPercent Value="35" />								<ReturnQuest NpcName="Senir Whitebeard" QuestName="Frostmane Hold" />							</If>
    <SetEnableVendoring Value="true"/>
    <SetVendorNpc NpcName="Innkeeper Belm"/>
    <SetVendorSlotsCount Value="2" />
    <SetEnableRepairing Value="true"/>
    <SetRepairNpc NpcName="Grawn Thromwyn"/>
    <SetRepairPercent Value="35" />

	</If>
    <If Condition="Data.HasActiveQuest('The Reports') == false and Data.HasCompletedQuest('Frostmane Hold') == true">		<SetResurrectionMode Mode="Corpse" />
        <TakeQuest NpcName="Senir Whitebeard" QuestName="The Reports" />
    </If>
    <If Condition="Data.HasActiveQuest('Operation Recombobulation') == false and Data.HasCompletedQuest('Operation Recombobulation') == false">
        <TakeQuest NpcName="Razzle Sprysprocket" QuestName="Operation Recombobulation" />
    </If>
	<AddBlackspot X="-4824.603515625" Y="-1010.6760864258" Z="502.1728515625" Range="2" />
    <If Condition="Data.HasActiveQuest('The Reports') == true">
        <ReturnQuest NpcName="Senator Barin Redstone" QuestName="The Reports" />
    </If>	
    <SetBaseSettings />
</ShadowProfile>
	    <If Condition="Data.HasActiveQuest('Evershine') == false and Data.HasCompletedQuest('Evershine') == false and Data.HasCompletedQuest('The Grizzled Den') == true and Data.HasCompletedQuest('Stocking Jetsteam') == true">

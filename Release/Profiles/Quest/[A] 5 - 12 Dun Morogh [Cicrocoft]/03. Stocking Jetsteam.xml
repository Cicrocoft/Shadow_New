<ShadowProfile Condition="Data.HasCompletedQuest('Stocking Jetsteam') == false or Data.HasCompletedQuest('The Grizzled Den') == false">
    <SetBaseSettings />	<!-- Restock Dun Morogh 5-12 (Kharanos)-->	<SetEnableRestockItems Value="true"/>	<ClearRestockItems/>	<!-- Food -->	<If Condition="GetMoney() &gt;= 700" >		<If Condition="Data.PlayerClass() == WoWClass.Rogue or Data.PlayerClass() == WoWClass.Warrior or Data.PlayerClass() == WoWClass.Hunter or Data.PlayerClass() == WoWClass.Warlock">			<AddRestockItem ItemName="Tough Hunk of Bread" NpcName="Innkeeper Belm" UnderAmount="5" BuyAmount="40"/>			<AddDoNotSellItem ItemName="Tough Hunk of Bread" />		</If>		<!-- Water -->		<If Condition="Data.PlayerClass() == WoWClass.Druid or Data.PlayerClass() == WoWClass.Hunter or Data.PlayerClass() == WoWClass.Warlock or Data.PlayerClass() == WoWClass.Paladin">			<AddRestockItem ItemName="Refreshing Spring Water" NpcName="Innkeeper Belm" UnderAmount="5" BuyAmount="40"/>			<AddDoNotSellItem ItemName="Refreshing Spring Water" />		</If>		<If Condition="Data.PlayerClass() == WoWClass.Priest">			<AddRestockItem ItemName="Refreshing Spring Water" NpcName="Innkeeper Belm" UnderAmount="3" BuyAmount="20"/>			<AddDoNotSellItem ItemName="Refreshing Spring Water" />		</If>	</If>				<!-- Bullets/Arrows -->	<If Condition="IsEquippedItemType('Gun') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Light Shot" NpcName="Kreg Bilmn" UnderAmount="100" BuyAmount="600"/>	</If>	<If Condition="IsEquippedItemType('Bow') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Rough Arrow" NpcName="Kreg Bilmn" UnderAmount="100" BuyAmount="600"/>	</If>	<If Condition="IsEquippedItemType('Crossbow') == true and Data.PlayerClass() == WoWClass.Hunter">		<AddRestockItem ItemName="Rough Arrow" NpcName="Kreg Bilmn" UnderAmount="100" BuyAmount="600"/>	</If>	<!-- End Restock -->
    <If Condition="Data.HasActiveQuest('Stocking Jetsteam') == false and Data.HasCompletedQuest('Stocking Jetsteam') == false">
        <TakeQuest NpcName="Pilot Bellowfiz" QuestName="Stocking Jetsteam" />
    </If>
    <If Condition="Data.HasActiveQuest('The Grizzled Den') == false and Data.HasCompletedQuest('The Grizzled Den') == false">
        <TakeQuest NpcName="Pilot Stonegear" QuestName="The Grizzled Den" />
    </If>


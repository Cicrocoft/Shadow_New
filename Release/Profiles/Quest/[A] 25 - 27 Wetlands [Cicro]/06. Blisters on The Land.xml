	<SetRestHPPercent Value="65" />
	<SetRestPWPercent Value="25" />
	<SetRestPetHPPercent Value="0" />
	<SetUseDrinkPercent Value="50" />
	<SetUseFoodPercent Value="69" />
    </If>	

		<AddRestockItem ItemName="Razor Arrow" NpcName="Gruham Rumdnul" UnderAmount="100" BuyAmount="800"/>
	</If>
	<If Condition="IsEquippedItemType('Crossbow') == true and Data.PlayerClass() == WoWClass.Hunter and UnitLevel('player') &gt;= 25">
		<AddRestockItem ItemName="Razor Arrow" NpcName="Gruham Rumdnul" UnderAmount="100" BuyAmount="800"/>
	</If>	
